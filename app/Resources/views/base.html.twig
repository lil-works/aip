<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    {{ sonata_seo_title() }}
    {{ sonata_seo_metadatas() }}

    {% image '@AppBundle/Resources/public/images/adress-ip.ico' %}
        <link rel="icon" type="image/x-icon" href="{{ asset_url }}" />
    {% endimage %}


    <!-- Bootstrap core CSS -->
    <link rel="stylesheet" href="{{ asset('assets/vendor/bootstrap/dist/css/bootstrap.min.css') }}">


    {#
    <!-- IE10 viewport hack for Surface/desktop Windows 8 bug -->
    <link href="../../assets/css/ie10-viewport-bug-workaround.css" rel="stylesheet">
    #}
    <!-- Custom styles for this template -->
    {% stylesheets '@AppBundle/Resources/public/css/jumbotron-narrow.css' filter="cssrewrite"  %}
    <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}


    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->


</head>

<body>

<div class="container">
    <div class="header clearfix">
        <nav>
            {{ knp_menu_render('main') }}
        </nav>
        <h3 class="text-muted">Adress Ip</h3>
    </div>

    {%  set adsenseId = (_context['adsenseId_' ~ app.request.locale])  %}
    <script async src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
    <!-- aip-es -->
    <ins class="adsbygoogle"
         style="display:block"
         data-ad-client="ca-pub-2677445990102112"
         data-ad-slot="{{ adsenseId  }}"
         data-ad-format="auto"></ins>
    <script>
        (adsbygoogle = window.adsbygoogle || []).push({});
    </script>

    <div class="jumbotron">
        {% block body %}{% endblock %}
        {% block javascripts %}{% endblock %}
    </div>

    <div class="row marketing">
        <div class="col-lg-6">
            <h4>GeoIp</h4>
            <p>
                <a href="{{ path('site_geoip') }}">{% trans %}Get ip location{% endtrans %}</a>
            </p>


            <h4>Whois</h4>
            <p>
                <a href="{{ path('site_whois') }}">{% trans %}Search in whois database{% endtrans %}</a>
            </p>

            <h4>Ping</h4>
            <p>
                <a href="{{ path('site_ping') }}">{% trans %}Ping an host from here{% endtrans %}</a>
            </p>


        </div>

        <div class="col-lg-6">
            <h4>{% trans %}PDF tools{% endtrans %}</h4>
            <p>
                <a href="{{ path('site_pdf_compressor') }}">{% trans %}Compress your PDF files{% endtrans %}</a>
            </p>
        </div>
    </div>

    <footer class="footer">
        <div class="row">
            <div class="col-lg-6">
                <p>&copy; 2017 lil-works â€¢ {{ app.environment }}</p>
            </div>
            <div class="col-lg-6">
                {{ knp_menu_render('lang') }}
            </div>
        </div>
    </footer>

</div> <!-- /container -->

{#
<!-- IE10 viewport hack for Surface/desktop Windows 8 bug -->
<script src="../../assets/js/ie10-viewport-bug-workaround.js"></script>
#}

<script src="{{ asset('assets/vendor/jquery/dist/jquery.js') }}"></script>
<script>
    (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
        (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
            m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
    })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

    ga('create', 'UA-790487-6', 'auto');
    ga('send', 'pageview');

</script>
</body>
</html>
